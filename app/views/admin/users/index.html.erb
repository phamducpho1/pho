<% provide(:title, t("users.index.title")) %>
<h1><%= t "users.index.title" %></h1>
<div class="table-responsive">
<table class="table">
    <thead>
      <tr>
        <th><%= t "admin.id" %></th>
        <th><%= t "admin.name" %></th>
        <th><%= t "admin.user.phone" %></th>
        <th><%= t "admin.user.address" %></th>
        <th><%= t "admin.createat" %></th>
        <th><%= t "admin.update" %></th>
        <th></th>
      </tr>
    </thead>
    <tbody>
        <%= render partial: "user", collection: @users %>
    </tbody>
    <%= will_paginate @users %>
</table>
</div>

